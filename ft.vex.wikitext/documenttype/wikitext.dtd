<!ENTITY % blockElements "heading|entity|image|link|acronym|linebreak|unescaped|CODE|DEFINITIONTERM|DIV|TABLEROW|TABLECELLHEADER|DEFINITIONLIST|TABLECELLNORMAL|QUOTE|FOOTNOTE|TIP|NOTE|PANEL|PREFORMATTED|NUMERICLIST|DEFINITIONITEM|TABLE|LISTITEM|PARAGRAPH|WARNING|BULLETEDLIST|INFORMATION" >
<!ENTITY % spanElements "INSERTED|CITATION|SUPERSCRIPT|MONOSPACE|BOLD|SPAN|CODE|DELETED|QUOTE|STRONG|SUBSCRIPT|UNDERLINED|LINK|EMPHASIS|ITALIC" >
<!ENTITY % defaultAttributes "
	id ID #IMPLIED
	title CDATA #IMPLIED
	language CDATA #IMPLIED
	class CDATA #IMPLIED
	style CDATA #IMPLIED
">

<!ELEMENT wikitext (%blockElements;|%spanElements;|#PCDATA)*>

<!ELEMENT heading (%spanElements;|#PCDATA)*>
<!ATTLIST heading 
	%defaultAttributes;
	level CDATA #REQUIRED
>

<!ELEMENT entity EMPTY>
<!ATTLIST entity 
	entity CDATA #REQUIRED
>

<!ELEMENT image EMPTY>
<!ATTLIST image 
	%defaultAttributes;
	url CDATA #REQUIRED
>

<!ELEMENT link (image|#PCDATA)>
<!ATTLIST link 
	%defaultAttributes;
	href CDATA #REQUIRED
>

<!ELEMENT acronym (#PCDATA)*>
<!ATTLIST acronym 
	definition CDATA #REQUIRED
>

<!ELEMENT linebreak EMPTY>

<!ELEMENT unescaped (#PCDATA)*>

<!ELEMENT CODE (%blockElements;|%spanElements;|#PCDATA)*>
<!ATTLIST CODE %defaultAttributes;>

